{
    "name": "osmscripts\/osmscripts",
    "description": "Command-line utility for creating OS-agnostic shell scripts in PHP.",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Vladislav Osmianskij",
            "email": "vlaosm@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "OsmScripts\\OsmScripts\\": "src\/"
        }
    },
    "require": {
        "php": ">=7.1",
        "osmscripts\/core": "1.*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "osmscripts": {
            "commands": {
                "create:package": "OsmScripts\\OsmScripts\\Commands\\CreatePackage",
                "create:script": "OsmScripts\\OsmScripts\\Commands\\CreateScript",
                "create:command": "OsmScripts\\OsmScripts\\Commands\\CreateCommand",
                "scripts": "OsmScripts\\OsmScripts\\Commands\\Scripts",
                "packages": "OsmScripts\\OsmScripts\\Commands\\Packages"
            },
            "variables": {
                "package": "Currently developed Composer package",
                "script": "Currently developed script"
            }
        }
    },
    "bin": [
        "osmscripts"
    ]
}