{
    "name": "osmscripts/osmscripts",
    "autoload": {
        "psr-4": {
            "OsmScripts\\OsmScripts\\": "src/"
        }
    },
    "require": {
        "php": ">=7.1",
        "osmscripts/core": "1.*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "osmscripts": {
            "commands": {
                "create:script": "OsmScripts\\OsmScripts\\Commands\\CreateScript"
            }
        }
    },
    "bin": [
        "osmscripts"
    ]
}